    <definition name="psycho" extends="base.definition">
        <put-attribute name="nav" value="/WEB-INF/jsp/psycho/nav.jsp"/>
        <put-attribute name="menu" value="/WEB-INF/jsp/psycho/base/menu.jsp"/>
        <put-attribute name="actions" value="/WEB-INF/jsp/psycho/base/actions.jsp"/>
        <put-attribute name="aside" value="/WEB-INF/jsp/psycho/aside.jsp"/>
        <put-attribute name="data" value="/WEB-INF/jsp/psycho/base/data.jsp"/>
        <put-attribute name="include" value="/WEB-INF/jsp/psycho/base/include.jsp"/>
    </definition>

    <definition name="psycho-view" extends="base.definition">
        <put-attribute name="nav" value="/WEB-INF/jsp/psycho/nav.jsp"/>
        <put-attribute name="menu" value="/WEB-INF/jsp/psycho/view/menu.jsp"/>
        <put-attribute name="actions" value="/WEB-INF/jsp/psycho/view/actions.jsp"/>
        <put-attribute name="aside" value="/WEB-INF/jsp/psycho/aside.jsp"/>
        <put-attribute name="data" value="/WEB-INF/jsp/psycho/view/data.jsp"/>
        <put-attribute name="include" value="/WEB-INF/jsp/psycho/view/include.jsp"/>
    </definition>

</tiles-definitions>