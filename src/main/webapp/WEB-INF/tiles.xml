    <definition name="discource" extends="base.definition">
        <put-attribute name="nav" value="/WEB-INF/jsp/discource/nav.jsp"/>
        <put-attribute name="menu" value="/WEB-INF/jsp/discource/base/menu.jsp"/>
        <put-attribute name="actions" value="/WEB-INF/jsp/discource/base/actions.jsp"/>
        <put-attribute name="aside" value="/WEB-INF/jsp/discource/aside.jsp"/>
        <put-attribute name="data" value="/WEB-INF/jsp/discource/base/data.jsp"/>
        <put-attribute name="include" value="/WEB-INF/jsp/discource/base/include.jsp"/>
    </definition>

