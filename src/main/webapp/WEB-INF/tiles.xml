        <put-attribute name="nav" value="/WEB-INF/jsp/discource/nav.jsp"/>
        <put-attribute name="menu" value="/WEB-INF/jsp/discource/view/menu.jsp"/>
        <put-attribute name="actions" value="/WEB-INF/jsp/discource/view/actions.jsp"/>
